#include <stdio.h>
int main()
{
	int a, b, c;
	
	printf("give first number\n");
	scanf("%d", &a);
	if (a/2 == 0) {
	printf(""%d", &a is a prime number");
	else 
	printf(""%d", &a is not a prime number");	
	}
	
	printf("give second number\n");
	scanf("%f", &b);
	if (a/2=int){
	printf(""%d", &a is a prime number");
	else 
	printf(""%d", &a is not a prime number");	
	}	
		
	printf("give third number\n");
	scanf(" %c", &c);
	if (a/2=int){
	printf(""%d", &a is a prime number");
	else 
	printf(""%d", &a is not a prime number");	
	}	
}
